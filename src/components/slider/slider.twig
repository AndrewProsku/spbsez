{% for item in items %}
    <div class="b-slider__item">
        <div class="b-slider__item-logo">
            <img src="{{ item.picture.source }}" alt="{{ item.picture.alt }}">
        </div>
        <div class="b-slider__item-desc">
            <p class="b-slider__item-title">
                {{ item.title }}
            </p>
            <p class="b-slider__item-text">
                {{ item.text }}
            </p>
        </div>
    </div>
{% endfor %}
