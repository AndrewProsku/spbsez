{
  "name": "kelnik-boilerplate",
  "version": "3.0.13",
  "author": "Kelnik Studios <demo@kelnik.ru>",
  "description": "",
  "main": "webpack.config.js",
  "keywords": [],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git@gitlab.kelnik.pro:internal/boilerplate.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel watch:gulp-dev watch:webpack",
    "watch:gulp-dev": "gulp dev",
    "watch:webpack": "webpack --watch --mode=development --config webpack.config.js",
    "production": "gulp production & webpack --mode=production --config webpack.config.prod.js",
    "eslint": "eslint -c .eslintrc src/**/*.js",
    "eslint-fix": "eslint -c .eslintrc src/**/*.js --fix",
    "stylelint": "stylelint src/**/**/*.scss",
    "stylelint-fix": "stylelint src/**/**/*.scss --fix",
    "lint": "npm run eslint && npm run stylelint",
    "createComponent": "node ./cli/createComponent.js",
    "removeComponent": "node ./cli/removeComponent.js",
    "generateCSS": "node ./cli/generatorCSS.js",
    "generatePageCSS": "node ./cli/generatorPageCSS.js",
    "createPage": "node ./cli/createPage.js",
    "removePage": "node ./cli/removePage.js",
    "renameComponent": "node ./cli/renameComponent.js",
    "renamePage": "node ./cli/renamePage.js",
    "add": "node ./cli/add.js"
  },
  "browserslist": [
    "ie >= 10",
    "ff >= 28",
    "Chrome >= 21",
    "Safari >= 7",
    "Opera >= 12"
  ],
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@glidejs/glide": "^3.2.4",
    "autoprefixer": "^9.1.5",
    "babel-loader": "^8.0.2",
    "babel-polyfill": "^6.26.0",
    "browser-sync": "^2.24.7",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "eslint": "^5.6.0",
    "file-loader": "^2.0.0",
    "ghooks": "^2.0.4",
    "gulp": "^4.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-fontgen": "^0.2.5",
    "gulp-plumber": "^1.2.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-rename": "^1.4.0",
    "gulp-responsive": "^2.11.0",
    "gulp-svgo": "^1.5.4",
    "gulp-twig": "^1.2.0",
    "gulp-typograf": "^3.1.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "posthtml": "^0.11.3",
    "posthtml-classes": "^0.2.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.5.0",
    "stylelint-scss": "^3.3.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "twig-loader": "^0.4.1",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "array-includes": "^3.0.3",
    "chosen-js": "^1.8.7",
    "detect-browser": "^3.0.1",
    "gmaps": "^0.4.24",
    "hammerjs": "^2.0.8",
    "inputmask": "^4.0.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "node-js-marker-clusterer": "^1.0.0",
    "object-fit-images": "^3.2.4",
    "perfect-scrollbar": "^1.4.0",
    "screenfull": "^3.3.3",
    "twig": "^1.10.5"
  }
}
